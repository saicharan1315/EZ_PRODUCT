class A:
    x = 10
    __y = 20
    
    def s(self):
        print("add")
    def work(self,x):
        self.x=x
        print(x)
        
class B(A):
    print("hi")
    
    
    
obj = B()
print(obj.x)
obj.s()
obj.work(50)
